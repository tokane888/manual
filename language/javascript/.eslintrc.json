{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["eslint:all"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "no-trailing-spaces": 2,
    "no-console": "off",
    "indent": ["error", 2]
  }
}